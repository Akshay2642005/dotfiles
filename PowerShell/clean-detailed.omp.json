{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#be8080>\u250f[</>{{ .UserName }}<#be8080>]</>",
          "foreground": "#808080",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#be8080>[</>{{ .HEAD }}<#be8080>]</>",
          "foreground": "#808080",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_virtual_envs": true
          },
          "template": "<#be8080>[</>{{.Venv }}<#be8080>]</>",
          "foreground": "#808080",
          "type": "python",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": "<#be8080>\u2516[</>{{ .Path }}<#be8080>]\nÎ»",
          "foreground": "#808080",
          "type": "path",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
