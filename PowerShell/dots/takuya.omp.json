{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#808080",
          "style": "plain",
          "template": "<#be8080>\u250f[</>{{ .UserName }}<#be8080>]</>",
          "type": "session"
        },
        {
          "foreground": "#808080",
          "style": "plain",
          "template": "<#be8080>[</>{{ .HEAD }}<#be8080>]</>",
          "type": "git"
        },


        {
          "foreground": "#808080",
          "style": "plain",
          "template": "<#be8080>[</>{{.Venv }}<#be8080>]</>",
          "type": "python",
          "properties": {
            "fetch_virtual_envs": true
          }

        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#808080",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "<#be8080>\u2516[</>{{ .Path }}<#be8080>]\nÎ»",
          "type": "path"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
